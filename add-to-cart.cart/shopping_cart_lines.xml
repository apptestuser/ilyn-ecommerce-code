<t name="Shopping Cart Lines" t-name="website_sale.cart_lines">
        <div t-if="not website_sale_order or not website_sale_order.website_order_line" class="js_cart_lines well well-lg">
          Your cart is empty!
        </div>
        
        
        <table class="table table-condensed js_cart_lines" style="margin-top: 50px;margin-bottom: 0px;" id="cart_products" t-if="website_sale_order and website_sale_order.website_order_line">
            
            <thead style="background:#FFC44D;color: black;border-left:4px solid #FFC44D;border-right:4px solid #FFC44D;">
                <tr>
                    <th width="150" style="border-right:1px solid #eccb9c;height: 53px !important;padding: 15px;"></th>
                    <th width="450" style="border-right:1px solid #eccb9c;padding: 15px;">Product Name</th>
                    <th width="100" style="border-right:1px solid #eccb9c;padding: 15px;">Price</th>
                    <th width="200" style="border-right:1px solid #eccb9c;padding: 15px;">Quantity</th>
                    <th width="100" style="border-right:1px solid #eccb9c;padding: 15px;">Subtotal</th>
                    <th width="50"></th>
                    
                    <!--<th width="100">Product Name</th>-->
                    <!--<th/>-->
                    <!--<th width="130" class="text-center">Quantity</th>-->
                    <!--<th width="100" class="text-center">Price</th>-->
                </tr>
            </thead>
            
            <tbody>
                <t t-foreach="website_sale_order.website_order_line" t-as="line">
                    
                    <tr style="border-left: 4px solid #F0F0F0;border-right: 4px solid #F0F0F0;border-bottom: 3px solid #F0F0F0;">
                        <td colspan="2" t-if="not line.product_id.product_tmpl_id"/>
                        <td align="center" t-if="line.product_id.product_tmpl_id" style="padding: 8px;">
                            <span t-field="line.product_id.image_small" t-options="{'widget': 'image', 'class': 'img-rounded'}"/>
                        </td>
                        
                        <td t-if="line.product_id.product_tmpl_id">
                            <div style="padding-top: 25px; padding-left:8px;">
                                <a t-attf-href="/shop/product/#{ slug(line.product_id.product_tmpl_id) }" style="color: black;font-size: 13px;font-family: Lato;text-decoration: none;">
                                    <strong t-esc="line.product_id.with_context(display_default_code=False).display_name"/>
                                </a>
                            </div>
                            <!--<div class="text-muted">-->
                            <!--    <t t-foreach="line.name.splitlines()[1:]" t-as="name_line">-->
                            <!--        <span><t t-esc="name_line"/></span><br/>-->
                            <!--    </t>-->
                            <!--</div>-->
                            
                        </td>
                        
                        
                        <td class="" id="td-price" name="price" style="padding-left: 17px;padding-top: 25px;font-weight: 500;">
                            <t t-if="(compute_currency(line.product_id.lst_price) - line.price_reduce ) &gt; 0.01  and website.get_current_pricelist().discount_policy=='without_discount'">
                                <del t-attf-class="#{'text-danger mr8'}" style="white-space: nowrap;" t-esc="compute_currency(line.product_id.website_public_price)" t-options="{'widget': 'monetary', 'display_currency': website.get_current_pricelist().currency_id, 'from_currency': website.currency_id}"/>
                            </t>
                            <span t-field="line.price_reduce_taxexcl" style="white-space: nowrap;" t-options="{'widget': 'monetary', 'from_currency': website_sale_order.pricelist_id.currency_id, 'display_currency': website.currency_id}" groups="sale.group_show_price_subtotal"/>
                            <span t-field="line.price_reduce_taxinc" style="white-space: nowrap;" t-options="{'widget': 'monetary', 'from_currency': website_sale_order.pricelist_id.currency_id, 'display_currency': website.currency_id}" groups="sale.group_show_price_total"/>
                        </td>
                        
                        <td class="text-center" id="td-qty" style="padding-top:20px">
                            <div class="input-group oe_website_spinner">
                                
                                <a t-attf-href="#" class="mb8 input-group-addon js_add_cart_json" data-no-instant="" >
                                    <i class="fa fa-minus"/>
                                </a>
                                
                                <input type="text" class="js_quantity form-control quantity" style="width: 130px;" t-att-data-line-id="line.id" t-att-data-product-id="line.product_id.id" t-att-value="int(line.product_uom_qty)"/>
                               
                                
                                <a t-attf-href="#" class="mb8 input-group-addon float_left js_add_cart_json" data-no-instant="" >
                                    <i class="fa fa-plus"/>
                                </a>
                               
                            </div>
                        </td>
                        
                        <td t-if="line.product_id.product_tmpl_id" style="padding-left: 17px;padding-top: 25px;font-weight: 500;"> 
                            00.00 ৳
                        </td>
                        
                        <td class="text-center" t-if="line.product_id.product_tmpl_id" style="padding-top: 25px;">
                            <a href="#" class="js_delete_product no-decoration"> <small><i style="color: #FF0002;font-size: 18px;font-weight: 500;" class="fa fa-trash-o"/></small></a>
                        </td>
                        
                        
                    </tr>
                    
                    
                </t>
            </tbody>
        </table>
        <div class="js_cart_lines">
            <t t-call="website_sale.total">
                <t t-set="extra_class" t-value="&quot;col-sm-4 col-sm-offset-8 col-xs-12&quot;"/>
            </t>
        </div>
    </t>

    
    